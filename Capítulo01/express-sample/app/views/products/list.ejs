<html>
    <body>
        <table border="solid">
            <tr>
                <td>id</td>
                <td>title</td>
                <td>description</td>
                <td>price</td>
            </tr>
            <%for(var i=0; i < books.length; i++) {%>
                <tr>
                    <td><%=books[i].id %></td>
                    <td><%=books[i].title %></td>
                    <td><%=books[i].description %></td>
                    <td><%=books[i].price %></td>
                </tr>
            <% } %>
        </table>
    </body>
</html>